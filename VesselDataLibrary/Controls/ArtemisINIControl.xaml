<UserControl x:Class="VesselDataLibrary.Controls.ArtemisINIControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ctl="clr-namespace:RussLibrary.Controls;assembly=RussLibrary"
	    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
	    xmlns:prop="clr-namespace:ArtemisModLoader.Properties;assembly=AMLLibrary"
             xmlns:resx="clr-namespace:AMLResources.Properties;assembly=AMLResources"
             xmlns:ValueConvert="clr-namespace:RussLibrary.ValueConverters;assembly=RussLibrary"
             mc:Ignorable="d" Name="uc">
    
    <UserControl.Resources>
        <prop:Settings x:Key="Settings" />
        <ValueConvert:NumericConverter x:Key="NumericConverter" />
        <ValueConvert:NullToBoolConverter x:Key="NullToBoolConverter" />
        <ValueConvert:BoolToVisibilityParameterConverter x:Key="BoolToVisibilityParameterConverter" />
    </UserControl.Resources>
    <UserControl.Background>
        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
            <GradientStop Color="{Binding Source={StaticResource Settings}, Path=Default.ArtemisINILight}" Offset="0" />
            <GradientStop Color="{Binding Source={StaticResource Settings}, Path=Default.ArtemisINIDark}" Offset="1" />
        </LinearGradientBrush>
    </UserControl.Background>
    <DockPanel>
        <xctk:DropDownButton DockPanel.Dock="Top" HorizontalAlignment="Right" HorizontalContentAlignment="Right">
            <xctk:DropDownButton.DropDownContent>

                <StackPanel Orientation="Horizontal">
                    <Label Content="Light Gradient:" VerticalAlignment="Center" />
                    <xctk:ColorPicker ShowRecentColors="True" SelectedColor="{Binding Source={StaticResource Settings}, Path=Default.ArtemisINILight}"/>
                    <Label Content="Dark Gradient:" />
                    <xctk:ColorPicker SelectedColor="{Binding Source={StaticResource Settings}, Path=Default.ArtemisINIDark}" />

                </StackPanel>
            </xctk:DropDownButton.DropDownContent>
        
        <StackPanel  Orientation="Horizontal" HorizontalAlignment="Right"  >
            <Button
                Visibility="{Binding Data.Changed, Mode=OneWay, ElementName=uc, Converter={StaticResource BoolToVisibilityParameterConverter}, ConverterParameter='Visible|Collapsed'}"
                IsEnabled="{Binding Data, Mode=OneWay, ElementName=uc, Converter={StaticResource NullToBoolConverter}}"
                HorizontalAlignment="Left" Content="Cancel Changes" IsCancel="True" Click="Cancel_Click" />
            <Button
                IsEnabled="{Binding Configuration, Mode=OneWay, ElementName=uc, Converter={StaticResource NullToBoolConverter}}"
                Margin="10,0,20,0"
                ToolTip="{x:Static resx:Resources.EditArtemisINI}" Click="EditArtemisINI_Click">
                <Button.Content>
                    <Image Height="32" Source="pack://application:,,,/VesselDataLibrary;component/Images/Notepad-32.png" />
                </Button.Content>
            </Button>
            <Button ToolTip="Open an Artemis INI File" HorizontalAlignment="Right" Click="Open_Click">
                <Button.Content>
                    <Image Height="32" Source="pack://application:,,,/VesselDataLibrary;component/Images/folder_open.png" />
                </Button.Content>
            </Button>
            <Button
                IsEnabled="{Binding Data, Mode=OneWay, ElementName=uc, Converter={StaticResource NullToBoolConverter}}"
                ToolTip="Save Artemis INI" HorizontalAlignment="Right" Click="Save_Click">
                <Button.Content>
                    <Image Height="32" Source="pack://application:,,,/VesselDataLibrary;component/Images/save.png" />
                </Button.Content>
            </Button>
            <Button
                IsEnabled="{Binding Data, Mode=OneWay, ElementName=uc, Converter={StaticResource NullToBoolConverter}}"
                ToolTip="{x:Static resx:Resources.SaveAs}" HorizontalAlignment="Right" Click="SaveAs_Click">
                <Button.Content>
                    <Image Height="32" Source="pack://application:,,,/VesselDataLibrary;component/Images/document_save_as.png" />
                </Button.Content>
            </Button>
        </StackPanel>
        </xctk:DropDownButton>
        <ScrollViewer
            IsEnabled="{Binding Data, Mode=OneWay, ElementName=uc, Converter={StaticResource NullToBoolConverter}}"
            DockPanel.Dock="Top"
            VerticalScrollBarVisibility="Auto">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <!-- overall settings -->
                <GroupBox
                BorderBrush="MediumAquamarine" BorderThickness="2"
                Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Stretch" >
                    <GroupBox.Header>
                        <TextBlock VerticalAlignment="Center" FontWeight="Bold" Text="General Settings" />
                    </GroupBox.Header>
                    <Grid HorizontalAlignment="Stretch" >
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition Width="70" />
                        </Grid.ColumnDefinitions>
                        <Label HorizontalAlignment="Right" Grid.Row="0" Grid.Column="0"  Content="Camera Pitch" FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.05" IsSnapToTickEnabled="True"
                            Minimum="-0.5" Maximum="0"
                            Grid.Row="0" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.CameraPitch, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox
                            Grid.Row="0" Grid.Column="2" Width="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.CameraPitch, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label HorizontalAlignment="Right" Grid.Row="1" Grid.Column="0"  Content="Camera Distance" FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="25" IsSnapToTickEnabled="True"
                            Minimum="50" Maximum="600"
                            Grid.Row="1" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.CameraDistance, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox
                            DisplayFormat="##0"
                            Grid.Row="1" Grid.Column="2"  MinWidth="70"
                            HorizontalAlignment="Stretch" Value="{Binding Path=Data.CameraDistance, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label HorizontalAlignment="Right" Grid.Row="2" Grid.Column="0"  Content="Network Port" FontWeight="Bold" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="####0"
                            Grid.Row="2" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch" Value="{Binding Path=Data.NetworkPort, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label HorizontalAlignment="Right" Grid.Row="3" Grid.Column="0"  Content="Jump Time Coefficient" FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            Minimum="0.01" Maximum="100.0"
                            Grid.Row="3" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.JumpTimeCoeff, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox DisplayFormat="##0.00"
                            Grid.Row="3" Grid.Column="2"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.JumpTimeCoeff, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                    </Grid>
                </GroupBox>

                <!-- Client-side settings -->
                <GroupBox
                    BorderBrush="DarkOrchid"  BorderThickness="2"
                    Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Stretch">
                    <GroupBox.Header>
                        <TextBlock VerticalAlignment="Center" FontWeight="Bold" Text="Client-side Settings" />
                    </GroupBox.Header>

                    <Grid HorizontalAlignment="Stretch" >
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />

                        </Grid.ColumnDefinitions>
                        <Label
                            HorizontalAlignment="Right" Grid.Row="0" Grid.Column="0"
                            Content="Force this Client to specific ship:" FontWeight="Bold" VerticalAlignment="Center" />

                        <ComboBox
                            MinWidth="70" HorizontalAlignment="Stretch"
                            Grid.Row="0" Grid.Column="1"
                            ItemsSource="{Binding Data.ValidShips, Mode=OneWay, ElementName=uc}"
                            SelectedItem="{Binding Path=Data.ShipRestriction, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock VerticalAlignment="Center" Text="{Binding Key, Mode=OneWay}" />
                                        <TextBlock VerticalAlignment="Center" Text=" - "  />
                                        <TextBlock VerticalAlignment="Center" Text="{Binding Value, Mode=OneWay}" />

                                    </StackPanel>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>




                        <Label
                            HorizontalAlignment="Right" Grid.Row="1" Grid.Column="0"
                            Content="Main Screen console allowed for Client" FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox
                            IsThreeState="True"
                            Grid.Row="1" Grid.Column="1"
                            IsChecked="{Binding Path=Data.ClientMainScreen, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="2" Grid.Column="0"
                            Content="Helm console allowed for Client" FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox
                            IsThreeState="True"
                            Grid.Row="2" Grid.Column="1"
                            IsChecked="{Binding Path=Data.ClientHelm, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                        HorizontalAlignment="Right" Grid.Row="3" Grid.Column="0"
                        Content="Weapon console allowed for Client" FontWeight="Bold"
                        VerticalAlignment="Center" />

                        <CheckBox
                        IsThreeState="True"
                        Grid.Row="3" Grid.Column="1"
                        IsChecked="{Binding Path=Data.ClientWeapon, Mode=TwoWay, ElementName=uc}"
                        VerticalAlignment="Center" />



                        <Label
                        HorizontalAlignment="Right" Grid.Row="4" Grid.Column="0"
                        Content="Engineer console allowed for Client" FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox
                        IsThreeState="True"
                        Grid.Row="4" Grid.Column="1"
                        IsChecked="{Binding Path=Data.ClientEngineer, Mode=TwoWay, ElementName=uc}"
                        VerticalAlignment="Center" />


                        <Label
                        HorizontalAlignment="Right" Grid.Row="5" Grid.Column="0"
                        Content="Science console allowed for Client"
                        FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox
                        IsThreeState="True"
                        Grid.Row="5" Grid.Column="1"
                        IsChecked="{Binding Path=Data.ClientScience, Mode=TwoWay, ElementName=uc}"
                        VerticalAlignment="Center" />



                        <Label
                        HorizontalAlignment="Right" Grid.Row="6" Grid.Column="0"
                        Content="Comms console allowed for Client"
                        FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox
                        IsThreeState="True"
                        Grid.Row="6" Grid.Column="1"
                        IsChecked="{Binding Path=Data.ClientComms, Mode=TwoWay, ElementName=uc}"
                        VerticalAlignment="Center" />



                        <Label
                        HorizontalAlignment="Right" Grid.Row="7" Grid.Column="0"
                        Content="Observer console allowed for Client" FontWeight="Bold"
                        VerticalAlignment="Center" />

                        <CheckBox
                        Grid.Row="7" Grid.Column="1"
                        IsChecked="{Binding Path=Data.ClientObserver, Mode=TwoWay, ElementName=uc}"
                        VerticalAlignment="Center" IsThreeState="True" />



                        <Label
                        HorizontalAlignment="Right" Grid.Row="8" Grid.Column="0"
                        Content="Use Joystick" FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox Grid.Row="8" Grid.Column="1"  IsChecked="{Binding Path=Data.UseJoystick, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label HorizontalAlignment="Right" Grid.Row="9" Grid.Column="0"  Content="Touch Screen" FontWeight="Bold" VerticalAlignment="Center" />

                        <CheckBox
                            Grid.Row="9" Grid.Column="1"  IsChecked="{Binding Path=Data.TouchScreen, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                        HorizontalAlignment="Right" Grid.Row="10" Grid.Column="0"
                        Content="IP Address to force used" FontWeight="Bold"
                        VerticalAlignment="Center" />

                        <TextBox
                            Grid.Row="10" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Text="{Binding Path=Data.ForceAddress, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />
                    </Grid>
                </GroupBox>
                <!-- Server side settings -->
                <GroupBox
                BorderBrush="Peru"  BorderThickness="2"
                Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Stretch">
                    <GroupBox.Header>
                        <TextBlock VerticalAlignment="Center" FontWeight="Bold" Text="Server-side Settings" />
                    </GroupBox.Header>

                    <Grid HorizontalAlignment="Stretch" >
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition Width="70"/>
                        </Grid.ColumnDefinitions>


                        <Label
                            HorizontalAlignment="Right" Grid.Row="0" Grid.Column="0"
                            Content="Lighting IP Address" FontWeight="Bold" VerticalAlignment="Center" />

                        <TextBox
                            Grid.Row="0" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Text="{Binding Path=Data.LightingIPAddress, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="1" Grid.Column="0"
                            Content="Station Energy" FontWeight="Bold" VerticalAlignment="Center" />


                        <Slider
                            TickPlacement="Both" TickFrequency="25" IsSnapToTickEnabled="True"
                            Minimum="500" Maximum="2000"
                            Grid.Row="1" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.StationEnergy, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox
                            DisplayFormat="###0"
                            MinNumber="500" MaxNumber="2000"
                            Grid.Row="1" Grid.Column="2"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.StationEnergy, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="2" Grid.Column="0"
                            Content="Player Shield Recharge Rate" FontWeight="Bold"
                            VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.01" IsSnapToTickEnabled="True"
                            Minimum="0.02" Maximum="2.0"
                            Grid.Row="2" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.PlayerShieldRechargeRate, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="0.00"
                            MinNumber="0.02" MaxNumber="2.0"
                            Grid.Row="2" Grid.Column="2"  MinWidth="70"
                            HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.PlayerShieldRechargeRate, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="3" Grid.Column="0"
                            Content="Enemy Shield Recharge Rate" FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.01" IsSnapToTickEnabled="True"
                            Minimum="0.02" Maximum="2.0"
                            Grid.Row="3" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnemyShieldRechargeRate, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="0.00"
                            Grid.Row="3" Grid.Column="2"
                            MinNumber="0.02" MaxNumber="2.0"
                            MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnemyShieldRechargeRate, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="4" Grid.Column="0"  Content="Station Sensor Range"
                            FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="5000"
                            Minimum="5000" Maximum="100000"
                            Grid.Row="4" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.StationSensorRange, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="#####0"
                            MinNumber="5000" MaxNumber="100000"
                            Grid.Row="4" Grid.Column="2"
                            MinWidth="70"
                            HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.StationSensorRange, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="5" Grid.Column="0"  Content="Player Sensor Range"
                            FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="5000"
                            Minimum="5000" Maximum="100000"
                            Grid.Row="5" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.PlayerSensorRange, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="#####0"
                            MinNumber="5000" MaxNumber="100000"
                            Grid.Row="5" Grid.Column="2"
                            MinWidth="70"
                            HorizontalAlignment="Stretch" Value="{Binding Path=Data.PlayerSensorRange, Mode=TwoWay, ElementName=uc}"
                            VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="6" Grid.Column="0"
                            Content="Energy Use Coefficient"
                            FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.1" IsSnapToTickEnabled="True"
                            Minimum="0.2" Maximum="3.0"
                            Grid.Row="6" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyUseCoeff, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="0.0"
                            MinNumber="0.2" MaxNumber="3.0"
                            Grid.Row="6" Grid.Column="2"  MinWidth="70"
                            HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyUseCoeff, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="7" Grid.Column="0"
                            Content="Energy Coefficient Beams" FontWeight="Bold" VerticalAlignment="Center" />
                        <Slider
                            TickPlacement="Both" TickFrequency="0.5" IsSnapToTickEnabled="True"
                            Minimum="1.0" Maximum="10.0"
                            Grid.Row="7" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffBeams, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="#0.0"
                            MinNumber="1.0" MaxNumber="10.0"
                            Grid.Row="7" Grid.Column="2"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffBeams, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="8" Grid.Column="0"
                            Content="Energy Coefficient Tubes" FontWeight="Bold" VerticalAlignment="Center" />
                        <Slider
                            TickPlacement="Both" TickFrequency="0.5" IsSnapToTickEnabled="True"
                            Minimum="1.0" Maximum="10.0"
                            Grid.Row="8" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffTubes, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox
                            DisplayFormat="#0.0"
                            MinNumber="1.0" MaxNumber="10.0"
                            Grid.Row="8" Grid.Column="2"
                            MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffTubes, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="9" Grid.Column="0"
                            Content="Energy Coefficient Tactical" FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.5" IsSnapToTickEnabled="True"
                            Minimum="1.0" Maximum="10.0"
                            Grid.Row="9" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffTactical, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox
                            DisplayFormat="#0.0"
                            MinNumber="1.0" MaxNumber="10.0"
                            Grid.Row="9" Grid.Column="2"
                            MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffTactical, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="10" Grid.Column="0"  Content="Energy Coefficient Maneuver"
                            FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.5" IsSnapToTickEnabled="True"
                            Minimum="1.0" Maximum="10.0"
                            Grid.Row="10" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffManeuver, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

                        <ctl:DecimalBox
                            DisplayFormat="#0.0"
                            MinNumber="1.0" MaxNumber="10.0"
                            Grid.Row="10" Grid.Column="2"  MinWidth="70"
                            HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffManeuver, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="11" Grid.Column="0"
                            Content="Energy Coefficient Impulse" FontWeight="Bold" VerticalAlignment="Center" />

                        <Slider
                            TickPlacement="Both" TickFrequency="0.5" IsSnapToTickEnabled="True"
                            Minimum="1.0" Maximum="10.0"
                            Grid.Row="11" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffImpulse, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />
                        <ctl:DecimalBox
                            DisplayFormat="#0.0"
                            MinNumber="1.0" MaxNumber="10.0"
                            Grid.Row="11" Grid.Column="2"  MinWidth="70"
                            HorizontalAlignment="Stretch" Value="{Binding Path=Data.EnergyCoeffImpulse, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />



                        <Label
                            HorizontalAlignment="Right" Grid.Row="12" Grid.Column="0"
                            Content="Energy Coefficient Warp"
                            FontWeight="Bold" VerticalAlignment="Center" />
                        <Slider
                            TickPlacement="Both" TickFrequency="0.5" IsSnapToTickEnabled="True"
                            Minimum="1.0" Maximum="10.0"
                            Grid.Row="12" Grid.Column="1"  MinWidth="70" HorizontalAlignment="Stretch"
                            Value="{Binding Path=Data.EnergyCoeffWarp, Mode=TwoWay, ElementName=uc}" VerticalAlignment="Center" />

